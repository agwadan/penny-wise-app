{
  "expo": {
    "name": "penny-wise-app",
    "slug": "penny-wise-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pennywise-icon.png",
    "scheme": "pennywiseapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.agwadan.pennywiseapp",
      "icon": "./assets/images/pennywise-icon.png",
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "backgroundColor": "#1A1A1A",
        "foregroundImage": "./assets/images/pennywise-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pennywise-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1A1A1A",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ee5fdbf3-79a4-4482-a89e-b6102e795534"
      }
    }
  }
}